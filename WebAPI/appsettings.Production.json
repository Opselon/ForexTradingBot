{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    // ✅ This will now correctly override the value from the base file.
    "DatabaseSettings": {
        "DatabaseProvider": "SqlServer"
    },
    "ConnectionStrings": {
        "DefaultConnection": "#{ConnectionString}#"
    },
    "TelegramPanel": {
        "BotToken": "#{TelegramBotToken}#",
        "UseWebhook": false,
        "EnableDebugMode": false
    },
    "TelegramUserApi": {
        "ApiId": "#{TelegramApiId}#",
        "ApiHash": "#{TelegramApiHash}#",
        "PhoneNumber": "#{TelegramPhoneNumber}#",
        "SessionPath": "C:\\Apps\\ForexTradingBot\\Session\\telegram_user.session",
        "VerificationCodeSource": "Disabled",
        "TwoFactorPasswordSource": "Disabled"
    },
    "CryptoPay": {
        "ApiToken": "#{CryptoPayApiToken}#",
        "IsTestnet": false
    }
}