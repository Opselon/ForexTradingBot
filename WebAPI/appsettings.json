{
    "Logging": {
        "LogLevel": {
            "Default": "Debug",
            "Microsoft.AspNetCore": "Debug"
        }
    },
    "AllowedHosts": "*",
    "DatabaseProvider": "SqlServer",
    "ConnectionStrings": {
        "DefaultConnection": "Server=localhost;Database=ForexBotDb;Trusted_Connection=True;MultipleActiveResultSets=true;Encrypt=False;TrustServerCertificate=False;"
    },
    "TelegramPanel": {
        "BotToken": "***REDACTED_BOT_TOKEN***",
        "UseWebhook": true,
        "WebhookAddress": "",
        "WebhookSecretToken": "",
        "PollingInterval": 0,
        "AdminUserIds": [ 1234567890 ],
        "EnableDebugMode": true
    },
    "TelegramUserApi": {
        "ApiId": 123456,
        "ApiHash": "***REDACTED_API_HASH***",
        "PhoneNumber": "***REDACTED_PHONE***",
        "SessionPath": "telegram_user.session",
        "VerificationCodeSource": "Console",
        "BotToken": "***REDACTED_BOT_TOKEN***",
        "TwoFactorPasswordSource": "Console"
    },
    "CryptoPay": {
        "ApiToken": "***REDACTED_CRYPTOPAY_TOKEN***",
        "BaseUrl": "https://pay.crypt.bot/api/",
        "IsTestnet": true,
        "WebhookSecretForCryptoPay": "***REDACTED_SECRET***"
    },
    "ForwardingRules": [
        {
            "BotToken": "***REDACTED_BOT_TOKEN***",
            "SourceChannelId": -1000000000000,
            "TargetChannelId": -1000000000001,
            "IsEnabled": true,
            "EditOptions": {
                "AppendText": "\n\nForwarded by MyBot",
                "ReplaceText": [
                    {
                        "Find": "OldText",
                        "ReplaceWith": "NewText"
                    }
                ],
                "RemoveLinks": false,
                "AllowedMessageTypes": [ "Text", "Photo", "Video" ]
            }
        }
    ],
    "HangfireSettings": {
        "StorageType": "SqlServer"
    }
}
